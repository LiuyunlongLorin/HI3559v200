-    andps          m5, m0               ; (0<y)?m5:0
-%endif
-
-%if USE_APPROXIMATION == 1
-    rsqrtps        m4, m4
-    mulps          m5, m4           ; m5 = p = Sxy_new*approx(1/sqrt(Syy) )
-%else
-    mulps          m5, m5
-    divps          m5, m4           ; m5 = p = Sxy_new*Sxy_new/Syy
-%endif
