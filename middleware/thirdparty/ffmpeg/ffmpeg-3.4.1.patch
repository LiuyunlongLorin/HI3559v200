-    pand           m0, m2                 ; (word) { Y0, Y1, ..., Y7 }
-    pand           m1, m2                 ; (word) { Y8, Y9, ..., Y15 }
-%else ; uyvy
-    psrlw          m0, 8                  ; (word) { Y0, Y1, ..., Y7 }
-    psrlw          m1, 8                  ; (word) { Y8, Y9, ..., Y15 }
-%endif ; yuyv/uyvy
-    packuswb       m0, m1                 ; (byte) { Y0, ..., Y15 }
-    mova    [dstq+wq], m0
-    add            wq, mmsize
-    jl .loop_%1
-    REP_RET
