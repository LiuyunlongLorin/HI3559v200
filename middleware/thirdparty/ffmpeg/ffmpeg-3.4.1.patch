-
-%macro WRITE_V_PIXEL 2
-%assign %%off 0
-
-%if %1 >= 8
-
-%rep %1/mmsize
-    movu     [%2+%%off], m0
-%assign %%off %%off+mmsize
-%endrep ; %1/mmsize
-
-%if mmsize == 16
-%if %1-%%off >= 8
-%if %1 > 16 && %1-%%off > 8
-    movu     [%2+%1-16], m0
-%assign %%off %1
-%else
-    movq     [%2+%%off], m0
-%assign %%off %%off+8
-%endif
-%endif ; %1-%%off >= 8
-%endif ; mmsize == 16
-
-%if %1-%%off >= 4
-%if %1 > 8 && %1-%%off > 4
-    movq      [%2+%1-8], m0
-%assign %%off %1
-%else
-    movd     [%2+%%off], m0
-%assign %%off %%off+4
-%endif
-%endif ; %1-%%off >= 4
-
-%else ; %1 < 8
-
-%rep %1/4
