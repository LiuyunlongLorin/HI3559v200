-    paddd         m0, m7
-%endif ; %1 == 16
-
-    ; clip, store
-    psrad         m0, 14 + %1 - %2
-%ifidn %3, X
-    movifnidn   dstq, dstmp
-%endif ; %3 == X
-%if %2 == 15
-    packssdw      m0, m0
-%ifnidn %3, X
-    movh [dstq+wq*(2>>wshr)], m0
-%else ; %3 == X
-    movd [dstq+wq*2], m0
-%endif ; %3 ==/!= X
-%else ; %2 == 19
-%if mmsize == 8
-    PMINSD_MMX    m0, m2, m4
-%elif cpuflag(sse4)
