-
-%macro H_EXTEND 2
-%assign %%n %1
-%rep 1+(%2-%1)/2
-%if cpuflag(avx2)
-cglobal emu_edge_hfix %+ %%n, 4, 4, 1, dst, dst_stride, start_x, bh
-%else
-cglobal emu_edge_hfix %+ %%n, 4, 5, 1, dst, dst_stride, start_x, bh, val
-%endif
-.loop_y:                                        ; do {
-    READ_V_PIXEL    %%n, [dstq+start_xq]        ;   $variable_regs = read($n)
